{
    "name": "hlx/auth",
    "description": "The Hlx Auth package.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-hash": "*",
        "hlx/collections": "^1.0",
        "hlx/contracts": "^1.0",
        "hlx/http": "^1.0",
        "hlx/macroable": "^1.0",
        "hlx/queue": "^1.0",
        "hlx/support": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hlx\\Auth\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "hlx/console": "Required to use the auth:clear-resets command (^1.0).",
        "hlx/queue": "Required to fire login / logout events (^1.0).",
        "hlx/session": "Required to use the session based guard (^1.0)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
