{
    "name": "hlx/http",
    "description": "The Hlx Http package.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "ext-filter": "*",
        "fruitcake/php-cors": "^1.2",
        "guzzlehttp/uri-template": "^1.0",
        "hlx/collections": "^1.0",
        "hlx/macroable": "^1.0",
        "hlx/session": "^1.0",
        "hlx/support": "^1.0",
        "symfony/http-foundation": "^6.2",
        "symfony/http-kernel": "^6.2",
        "symfony/mime": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "Hlx\\Http\\": ""
        }
    },
    "suggest": {
        "ext-gd": "Required to use Hlx\\Http\\Testing\\FileFactory::image().",
        "guzzlehttp/guzzle": "Required to use the HTTP Client (^7.5)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
