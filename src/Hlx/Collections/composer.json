{
    "name": "hlx/collections",
    "description": "The Hlx Collections package.",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "hlx/conditionable": "^1.0",
        "hlx/contracts": "^1.0",
        "hlx/macroable": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Hlx\\Support\\": ""
        },
        "files": [
            "helpers.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "suggest": {
        "symfony/var-dumper": "Required to use the dump method (^6.2)."
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
